        "babel-preset-jest": "^25.1.0",
        "enzyme-to-json": "^3.3.4",
        "eslint": "^5.3.0",
        "eslint-plugin-react": "^7.11.1",
        "express": "^4.16.2",
        "moment": "^2.22.2",
        "nock": "^10.0.0",
        "nodemon": "^1.18.3",
        "react": "^16.2.0",
        "react-dom": "^16.2.0",
        "react-redux": "^5.0.7",
        "react-tippy": "^1.2.2",
        "redux": "^4.0.0",
        "redux-thunk": "^2.3.0",
        "superagent": "^3.8.3",
        "terminal-sweetness": "^1.0.3",
        "webpack": "^3.10.0",
